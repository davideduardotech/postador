<%- include('../../../partials/head', {title: "Planos" }) %>

<body class="bg-gray-100">
  <%- include('../../../partials/app/sidebar') %>

  <div class="xl:ml-64 pb-14">
    <%- include('../../../partials/app/nav', {user: user, title: "Planos"}) %>

    <main class="px-8 md:px-12 py-4 pb-20 fade-in">
      <div class="z-50 fixed top-0 left-0 right-0 bottom-0 flex items-center w-full h-full p-4 bg-gray-800 bg-opacity-50 fade-in hidden" id="popup-update-plan">
        <div class="max-w-xl bg-white w-full mx-auto rounded-xl fire-sans">
          <div class="flex justify-between items-center px-6 py-3 border-b border-gray-200">
            <div class="text-lg font-semibold">Editar plano</div>
            <button onclick="closeUpdatePlan()" class="fill-current font-4xl font-bold">×</button>
          </div>

          <div class="p-6 flex-grow h-96 overflow-y-auto">
            <div class="flex flex-wrap flex-col">
              <div class="grid grid-cols-2 gap-x-3 mt-2">
                <div>
                  <label for="title" class="font-medium text-sm">Titulo</label>
                  <div class="mt-1.5">
                    <input id="title" name="title" type="text" placeholder="Titulo do plano" class="block w-full text-sm rounded-lg px-2 py-1.5 border border-gray-300 placeholder:text-gray-400 sm:leading-6">
                  </div>
                </div>

                <div>
                  <label for="value-month" class="font-medium text-sm">Valor por mês</label>
                  <div class="mt-1.5">
                    <input id="value-month" name="value-month" type="text" placeholder="Valor do plano" class="block w-full text-sm rounded-lg px-2 py-1.5 border border-gray-300 placeholder:text-gray-400 sm:leading-6">
                  </div>
                </div>
              </div>
  
              <div class="mt-6">
                <label for="description" class="font-medium text-sm">Descrição</label>
                <div class="mt-1.5">
                  <textarea id="description" name="description" placeholder="Descrição do plano" class="block w-full text-sm rounded-lg px-2 py-1.5 border border-gray-300 placeholder:text-gray-400 sm:leading-6"></textarea>
                </div>
              </div>

              <div class="mt-6">
                <label for="marked" class="font-medium text-sm">Marcar como melhor opção</label>
                <div class="mt-1.5">
                  <select id="marked" name="marked" class="block w-full text-sm rounded-lg px-1.5 py-2 border border-gray-300 placeholder:text-gray-400 sm:leading-6">
                    <option value="true">Sim</option>
                    <option value="false">Não</option>
                  </select>
                </div>
              </div>

              <div class="grid grid-cols-2 gap-x-3 mt-6">
                <div>
                  <label for="quantity-accounts" class="font-medium text-sm">Qtd. de perfis</label>
                  <div class="mt-1.5">
                    <input id="quantity-accounts" name="quantity-accounts" type="number" placeholder="Qtd." class="block w-full text-sm rounded-lg px-2 py-1.5 border border-gray-300 placeholder:text-gray-400 sm:leading-6">
                  </div>
                </div>

                <div>
                  <label for="quantity-groups" class="font-medium text-sm">Qtd. de grupos</label>
                  <div class="mt-1.5">
                    <input id="quantity-groups" name="quantity-groups" type="number" placeholder="Qtd." class="block w-full text-sm rounded-lg px-2 py-1.5 border border-gray-300 placeholder:text-gray-400 sm:leading-6">
                  </div>
                </div>
              </div>

              <div class="grid grid-cols-2 gap-x-3 mt-6">
                <div>
                  <label for="quantity-pages" class="font-medium text-sm">Qtd. de páginas</label>
                  <div class="mt-1.5">
                    <input id="quantity-pages" name="quantity-pages" type="number" placeholder="Qtd." class="block w-full text-sm rounded-lg px-2 py-1.5 border border-gray-300 placeholder:text-gray-400 sm:leading-6">
                  </div>
                </div>

                <div>
                  <label for="quantity-schedule" class="font-medium text-sm">Qtd. de posts pendentes</label>
                  <div class="mt-1.5">
                    <input id="quantity-schedule" name="quantity-schedule" type="number" placeholder="Qtd." class="block w-full text-sm rounded-lg px-2 py-1.5 border border-gray-300 placeholder:text-gray-400 sm:leading-6">
                  </div>
                </div>
              </div>

              <div class="grid grid-cols-2 gap-x-3 mt-6">
                <div>
                  <label for="quantity-reply-posts" class="font-medium text-sm">Qtd. de posts com comentários automáticos</label>
                  <div class="mt-1.5">
                    <input id="quantity-reply-posts" name="quantity-reply-posts" type="number" placeholder="Qtd." class="block w-full text-sm rounded-lg px-2 py-1.5 border border-gray-300 placeholder:text-gray-400 sm:leading-6">
                  </div>
                </div>

                <div>
                  <label for="quantity-comments" class="font-medium text-sm">Qtd. de comentários respondidos</label>
                  <div class="mt-1.5">
                    <input id="quantity-comments" name="quantity-comments" type="number" placeholder="Qtd." class="block w-full text-sm rounded-lg px-2 py-1.5 border border-gray-300 placeholder:text-gray-400 sm:leading-6">
                  </div>
                </div>
              </div>

              <div class="grid grid-cols-2 gap-x-3 mt-6">
                <div>
                  <label for="quantity-sets" class="font-medium text-sm">Qtd. de conjuntos</label>
                  <div class="mt-1.5">
                    <input id="quantity-sets" name="quantity-sets" type="number" placeholder="Qtd." class="block w-full text-sm rounded-lg px-2 py-1.5 border border-gray-300 placeholder:text-gray-400 sm:leading-6">
                  </div>
                </div>

                <div>
                  <label for="select-all" class="font-medium text-sm">Selecionar todos</label>
                  <div class="mt-1.5">
                    <select id="select-all" name="select-all" class="block w-full text-sm rounded-lg px-1.5 py-2 border border-gray-300 placeholder:text-gray-400 sm:leading-6">
                      <option value="true">Sim</option>
                      <option value="false">Não</option>
                    </select>
                  </div>
                </div>
              </div>

              <div class="grid grid-cols-2 gap-x-3 mt-6">
                <div>
                  <label for="standard-answers" class="font-medium text-sm">Respostas padrões</label>
                  <div class="mt-1.5">
                    <select id="standard-answers" name="standard-answers" class="block w-full text-sm rounded-lg px-1.5 py-2 border border-gray-300 placeholder:text-gray-400 sm:leading-6">
                      <option value="true">Sim</option>
                      <option value="false">Não</option>
                    </select>
                  </div>
                </div>
              </div>
            </div>
          </div>

          <div class="px-6 py-3 border-t dark:border-gray-800 gap-x-2 flex justify-end">
            <button onclick="closeUpdatePlan()" class="py-2 px-4 inline-block text-center rounded-lg leading-5 text-gray-800 bg-gray-100 border border-gray-200 hover:text-gray-900 hover:bg-gray-200 hover:ring-0 hover:border-gray-200 focus:bg-gray-200 focus:border-gray-200 focus:outline-none focus:ring-0 ltr:mr-2 rtl:ml-2">Fechar</button>
            <button onclick="updatePlan()" class="py-2 px-4 inline-block text-center rounded-lg leading-5 text-gray-100 bg-purple font-medium hover:text-white">Atualizar</button>
          </div>
        </div>
      </div>

      <div>
        <div class="flex mt-4">
          <div class="flex space-x-4 items-center">
            <h2 class="text-xl font-semibold text-gray-900">Planos</h2>
            <div class="h-6 w-px bg-gray-400" aria-hidden="true"></div>
            <span class="text-md text-gray-900"><%= plans.length %> encontrados</span>
          </div>
        </div>
        
        <% if (plans.length > 0) { %>
          <div class="mt-8">
            <ul role="list" class="grid grid-cols-1 gap-6 sm:grid-cols-2 lg:grid-cols-4">
              <% plans.forEach(function(plan) { %>
                <li class="col-span-1 divide-y divide-gray-200 rounded-lg bg-white shadow border">
                  <div class="px-6">
                    <div class="mt-6">
                      <div>
                        <div class="flex items-center truncate text-xl font-medium text-purple">                       
                          <%= plan.title %>

                          <% if (plan.marked) { %>
                            <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="w-6 h-6 ml-1.5 text-purple">
                              <path stroke-linecap="round" stroke-linejoin="round" d="M11.48 3.499a.562.562 0 011.04 0l2.125 5.111a.563.563 0 00.475.345l5.518.442c.499.04.701.663.321.988l-4.204 3.602a.563.563 0 00-.182.557l1.285 5.385a.562.562 0 01-.84.61l-4.725-2.885a.563.563 0 00-.586 0L6.982 20.54a.562.562 0 01-.84-.61l1.285-5.386a.562.562 0 00-.182-.557l-4.204-3.602a.563.563 0 01.321-.988l5.518-.442a.563.563 0 00.475-.345L11.48 3.5z" />
                            </svg>
                          <% } %> 
                        </div>
                        <span class="text-sm"><%= plan.description %></span>
                      </div>

                      <p class="mt-4 flex items-baseline gap-x-1">
                        <span class="text-2xl font-bold tracking-tight text-gray-900">R$<%= plan.valueMonth %></span>
                        <span class="text-sm font-semibold leading-6 text-gray-600">/mês</span>
                      </p>
                      
                      <ul role="list" class="mt-6 mb-8 space-y-3 text-sm leading-6 text-gray-600">
                        <li class="flex gap-x-3">
                          <svg class="h-6 w-5 flex-none" viewBox="0 0 20 20" fill="#752a7a"
                            aria-hidden="true">
                            <path fill-rule="evenodd"
                              d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.857-9.809a.75.75 0 00-1.214-.882l-3.483 4.79-1.88-1.88a.75.75 0 10-1.06 1.061l2.5 2.5a.75.75 0 001.137-.089l4-5.5z"
                              clip-rule="evenodd" />
                          </svg>
                          Publique no Facebook e Instagram
                        </li>
  
