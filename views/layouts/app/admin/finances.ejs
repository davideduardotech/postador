<%- include('../../../partials/head', {title: "Financeiro" }) %>

<body class="bg-gray-100">
  <%- include('../../../partials/app/sidebar') %>

  <div class="xl:ml-64">
    <%- include('../../../partials/app/nav', {user: user, title: "Financeiro"}) %>

    <main class="px-8 md:px-12 py-4 pb-20 fade-in">
      <% if (finances.length > 0) { %>
        <div class="flex justify-between items-center mt-4">
          <div class="flex space-x-4 items-center">
            <h2 class="text-xl fire-sans font-semibold text-gray-900">Faturamento</h2>
          </div>
        </div>

        <div class="py-6 bg-gray-50 border border-gray-300 rounded-xl mt-5">
          <div class="px-6">
            <div class="w-full overflow-x-auto">
              <table class="w-full min-w-max">
                <thead>
                  <tr class="text-left">
                    <th class="p-0 border-b border-neutral-100">
                      <div class="pb-3.5"><span class="text-sm text-gray-400 font-medium">ID (MP)</span></div>
                    </th>
                    <th class="p-0 border-b border-neutral-100">
                      <div class="pb-3.5"><span class="text-sm text-gray-400 font-medium">Usuário</span></div>
                    </th>
                    <th class="p-0 border-b border-neutral-100">
                      <div class="pb-3.5"><span class="text-sm text-gray-400 font-medium">Plano</span></div>
                    </th>
                    <th class="p-0 border-b border-neutral-100">
                      <div class="pb-3.5"><span class="text-sm text-gray-400 font-medium">Forma de pagamento</span></div>
                    </th>
                    <th class="p-0 border-b border-neutral-100">
                      <div class="pb-3.5"><span class="text-sm text-gray-400 font-medium">Valor</span></div>
                    </th>
                    <th class="p-0 border-b border-neutral-100">
                      <div class="pb-3.5"><span class="text-sm text-gray-400 font-medium">Data</span></div>
                    </th>
                  </tr>
                </thead>
                <tbody>
                  <% finances.forEach(finance => { %>
                    <tr>
                      <td class="py-1.5 pr-4 border-b border-neutral-100">
                        <span class="text-sm font-semibold"><%= finance.idPayment %></span>
                      </td>
                      <td class="py-1.5 pr-4 border-b border-neutral-100">
                        <div class="flex items-center">
                          <% if (finance.avatarUser.length > 0) { %>
                            <img class="h-10 w-10 mr-2 rounded-full object-cover" src="/img/avatars/<%= finance.avatarUser %>" alt="">
                          <% } else { %>
                            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor" class="h-10 w-10 text-purple mr-2">
                              <path fill-rule="evenodd" d="M18.685 19.097A9.723 9.723 0 0021.75 12c0-5.385-4.365-9.75-9.75-9.75S2.25 6.615 2.25 12a9.723 9.723 0 003.065 7.097A9.716 9.716 0 0012 21.75a9.716 9.716 0 006.685-2.653zm-12.54-1.285A7.486 7.486 0 0112 15a7.486 7.486 0 015.855 2.812A8.224 8.224 0 0112 20.25a8.224 8.224 0 01-5.855-2.438zM15.75 9a3.75 3.75 0 11-7.5 0 3.75 3.75 0 017.5 0z" clip-rule="evenodd" />
                            </svg>                                                                      
                          <% } %>
                          
                          <span class="text-sm"><%= finance.nameUser %></span>
                        </div>
                      </td>
                      <td class="py-1.5 pr-4 border-b border-neutral-100">
                        <span class="text-sm text-purple font-semibold"><%= finance.titlePlan %></span>
                      </td>
                      <td class="py-1.5 pr-4 border-b border-neutral-100">
                        <span class="text-sm"><%= (finance.typePayment == "pix") ? "Pix" : "Cartão de crédito" %></span>
                      </td>
                      <td class="py-1.5 pr-4 border-b border-neutral-100">
                        <span class="text-sm text-green-600 font-semibold">R$<%= finance.amount %>,00</span>
                      </td>
                      <td class="py-1.5 pr-4 border-b border-neutral-100">
                        <span class="text-sm"><%= finance.createdAtFormatted %></span>
                      </td>
                    </tr>
                  <% }) %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      <% } else { %>
        <div class="flex flex-col items-center py-44 h-full">
          <svg class="h-72" width="888" height="545" viewBox="0 0 888 545" fill="none" xmlns="http://www.w3.org/2000/svg">
